//Palindrome Number
//Given an integer x, return true if x is a palindrome, and false otherwise.
/**
 * @param {number} x
 * @return {boolean}
 */
var isPalindrome = function (x) {
  //convert x to string, string to array, reverse the array, and join back to string
  let reverseX = x.toString().split('').reverse().join('')
  //use == operator to compare x and reversed string for allowing type coercion
  if (x == reverseX) {
    return true
  }
  return false
}
